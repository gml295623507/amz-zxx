ar、bpmn' }))\r\n        return false\r\n      }\r\n    },\r\n    // 上传成功\r\n    successHandle (res, file, fileList) {\r\n      if (res.code !== 0) {\r\n        return this.$message.error(res.msg)\r\n      }\r\n      this.$message({\r\n        message: this.$t('prompt.success'),\r\n        type: 'success',\r\n        duration: 500,\r\n        onClose: () => {\r\n          this.visible = false\r\n          this.$emit('refreshDataList')\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n"]}]}